{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "imageOfTheDay": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/api/image-of-the-day"
            },
            "backendUri": "https://www.bing.com/HPImageArchive.aspx?format=js&idx=0&n={request.querystring.count}&mkt=en-US"
        }
    }
}